    @RenderSection("scripts", required: false)
    <script type="text/javascript">
        var L_curdate = "";
        $(document).ready(function () {
            $("#loading").css("display", "none");
            var IsAlertsCenterOpen = 0;
            var L_d = new Date();
            var L_month = L_d.getMonth() + 1;
            var L_day = L_d.getDate();
            L_curdate = L_d.getFullYear() + '' + (L_month < 10 ? '0' : '') + L_month + '' + (L_day < 10 ? '0' : '') + '' + L_day + '' + L_d.getHours() + '' + L_d.getMinutes() + '' + L_d.getSeconds();

            $('[data-toggle="tooltip"],[data-toggle="modal"]').tooltip();
            $('#sidebarCollapse').on('click', function () {
                $('#sidebar').toggleClass('active');
                $('.contnt').toggleClass('min-content');
            });
            $('.custom-toggle.two').on('click', function () {
                $('#sidebar').toggleClass('active');
                $('.contnt').toggleClass('min-content');
            });
        });
        function getParameterByName(name) {
            var url = window.location.href;
            name = name.replace(/[\[\]]/g, '\\$&');
            var regex = new RegExp('[?&]' + name + '(=([^&#]*)|&|#|$)'),
                results = regex.exec(url);
            if (!results) return null;
            if (!results[2]) return '';
            return decodeURIComponent(results[2].replace(/\+/g, ' '));
        }
        function isNumberKey(evt) {
            var charCode = (evt.which) ? evt.which : evt.keyCode;
            if (charCode != 46 && charCode > 31 && (charCode < 48 || charCode > 57))
                return false;
            else
                return true;
        }

    </script>
